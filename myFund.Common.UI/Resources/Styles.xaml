    <SolidColorBrush x:Key="DefaultThemeColor">#FF086F9E</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultLabelColor">#FF7F7F7F</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultValueColor">#FF333333</SolidColorBrush>

    <Style x:Key="ShowHideStockButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Background" Value="White" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Height" Value="45" />
        <Setter Property="Width" Value="45" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="BorderBrush" Value="Black" />
    </Style>

    <Style TargetType="{x:Type controls:ModuleItemControl}">
        <Setter Property="Margin" Value="0,10,0,10" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Height" Value="100" />
    </Style>

    <Style x:Key="NavigationItemNameStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="TextAlignment" Value="Center" />
        <Setter Property="FontSize" Value="35" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style x:Key="NavigationItemButtonStyle"
           BasedOn="{StaticResource AccentedSquareButtonStyle}"
           TargetType="{x:Type Button}">
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
    </Style>

    <Style x:Key="StockTitleStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="FontSize" Value="40" />
        <Setter Property="Foreground" Value="Black" />
    </Style>

    <Style x:Key="DefaultPropertyHeaderStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="Margin" Value="0,0,0,2" />
        <Setter Property="FontSize" Value="15" />
        <Setter Property="Foreground" Value="{StaticResource DefaultLabelColor}" />
    </Style>

    <Style x:Key="DefaultPropertyValueStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="FontSize" Value="35" />
        <Setter Property="Foreground" Value="{StaticResource DefaultValueColor}" />
    </Style>
</ResourceDictionary>